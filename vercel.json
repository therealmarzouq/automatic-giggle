{
  "routes": [
    {
      "src": "/api",
      "dest": "https://automatic-giggle.vercel.app/api/"
    },

    {
      "src": "/api/(.*?)([.]\\w{2,5})(\\?[^/]+)?",
      "dest": "https://automatic-giggle.vercel.app/api/$1$2$3"
    },
    {
      "src": "/api/([\\S\\s]+)",
      "dest": "https://automatic-giggle.vercel.app/api/$1/"
    }
  ]
}
